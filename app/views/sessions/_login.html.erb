<h3>Log In</h3>
<div class="error">
  <%=flash[:error]%>
</div>
  <p><%=flash[:login_message]%></p>
<%= form_for :session, url: login_path do |f| %>
  <%= f.text_field :email, placeholder: 'Email' %>
  <%= f.password_field :password, placeholder: "Password" %>
  <%= f.submit "Log In", :class => "button" %>
<% end %>
